{
	"name": "wban-vaults",
	"version": "0.0.1",
	"description": "wBAN Vaults Strategies",
	"author": "Wrap That Potassium <wrap-that-potassium@protonmail.com>",
	"license": "AGPL-3.0-or-later",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"repository": {
		"type": "git",
		"url": "https://github.com/wBanano/wban-vaults"
	},
	"keywords": [
		"Banano",
		"Wrapped Banano",
		"DeFi",
		"Farms",
		"Vaults"
	],
	"files": [
		"contracts",
		"dist"
	],
	"engines": {
		"node": ">=10"
	},
	"devDependencies": {
		"@ethersproject/abi": "^5.5.3",
		"@ethersproject/bytes": "^5.5.0",
		"@ethersproject/providers": "^5.5.3",
		"@nomicfoundation/hardhat-chai-matchers": "^1.0.4",
		"@nomiclabs/hardhat-ethers": "^2.2.1",
		"@nomiclabs/hardhat-etherscan": "^3.1.2",
		"@nomiclabs/hardhat-solhint": "^2.0.1",
		"@openzeppelin/contracts": "^4.8.0",
		"@typechain/ethers-v5": "^10.1.1",
		"@typechain/hardhat": "^6.1.4",
		"@types/bn.js": "^5.1.1",
		"@types/chai": "^4.3.4",
		"@types/keccak": "^3.0.1",
		"@types/mocha": "^10.0.0",
		"@types/node": "^18.11.9",
		"chai": "^4.3.7",
		"copyfiles": "^2.4.1",
		"dotenv": "^8.2.0",
		"ethers": "^5.5.3",
		"hardhat": "^2.12.5",
		"hardhat-abi-exporter": "^2.10.1",
		"hardhat-contract-sizer": "^2.6.1",
		"hardhat-dependency-compiler": "^1.1.3",
		"hardhat-log-remover": "^2.0.2",
		"hardhat-preprocessor": "^0.1.5",
		"hardhat-spdx-license-identifier": "^2.1.0",
		"istanbul": "^0.4.5",
		"keccak": "^3.0.2",
		"lodash": "^4.17.21",
		"mocha": "^10.1.0",
		"prettier": "^2.2.1",
		"prettier-plugin-solidity": "^1.0.0-beta.10",
		"rimraf": "^3.0.0",
		"solhint": "^3.3.7",
		"solidity-coverage": "0.8.2",
		"ts-node": "^10.9.1",
		"typechain": "^8.1.1",
		"typescript": "^4.8.4",
		"@uniswap/v2-core": "^1.0.1",
		"@uniswap/v2-periphery": "^1.1.0-beta.0"
	},
	"resolutions": {
		"@nomiclabs/hardhat-solhint/solhint": "^3.3.2"
	},
	"scripts": {
		"clean": "rimraf ./artifacts/ ./flatten/ ./coverage/ ./coverage.json ./cache/ ./dist",
		"compile": "hardhat compile && yarn tsc",
		"postcompile": "copyfiles -u 2 \"artifacts/typechain/**/*.d.ts\" dist",
		"pretest": "yarn compile",
		"test": "hardhat test",
		"prepublishOnly": "yarn test",
		"check": "hardhat check",
		"solhint": "solhint \"contracts/**/*.sol\" -f unix",
		"lint": "prettier --check \"contracts/**/*.{sol,ts,js}\"",
		"prettier": "prettier --write \"contracts/**/*.sol\" \"test/**/*.{ts,js}\"",
		"coverage": "hardhat coverage && istanbul check-coverage ./coverage.json --statements 100 --branches 91.6 --functions 100 --lines 100",
		"node:watch": "hardhat node --watch"
	}
}
